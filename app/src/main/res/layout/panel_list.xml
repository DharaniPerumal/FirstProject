<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="4dp"
    android:background="#fff"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--    <include-->
    <!--        android:id="@+id/to_service_anel"-->
    <!--        layout="@layout/toolbar"/>-->

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="20dp"
        android:padding="5dp"
        android:background="#ffff"
        android:elevation="4dp"
        android:layout_margin="20dp"
        android:id="@+id/text_panel">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FDF8F8"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/info"
                android:layout_width="55dp"
                android:layout_gravity="right"
                android:layout_height="33dp"
                android:src="@drawable/ic_baseline_info_24" />

            <TextView
                android:id="@+id/username"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="username"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:textColor="#00695C"
                android:textSize="20dp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/ic_baseline_directions_bike_24" />

                <TextView
                    android:id="@+id/vehicle_no"
                    android:layout_width="132dp"
                    android:layout_height="match_parent"
                    android:text="vehicle no."
                    android:textColor="#000"
                    android:textSize="18dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_baseline_local_phone_24"/>

                <TextView
                    android:id="@+id/mobile_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mobile 1"
                    android:textSize="18dp"
                    android:textColor="#000"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_baseline_local_phone_24"/>

                <TextView
                    android:id="@+id/mobile_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mobile 2"
                    android:textSize="18dp"
                    android:layout_marginLeft="10dp"
                    android:textColor="#000"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_baseline_home_24"/>

                <TextView
                    android:id="@+id/place"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Place"
                    android:textSize="18dp"
                    android:layout_marginLeft="10dp"
                    android:textColor="#000"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_baseline_edit_calendar_24"/>

                <TextView
                    android:id="@+id/available_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Available Date"
                    android:textSize="18dp"
                    android:layout_marginLeft="10dp"
                    android:textColor="#000"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_baseline_menu_book_24"/>

                <TextView
                    android:id="@+id/serial_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Serial_no."
                    android:textSize="18dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:textColor="#000"/>

            </LinearLayout>

            <Button
                android:id="@+id/Service_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Completed"
                android:backgroundTint="#00695C"/>

            <LinearLayout
                android:id="@+id/sp_hiddenbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone"
                android:layout_gravity="center"
                android:gravity="center">

                <Button
                    android:id="@+id/btn_complete"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="Complete"
                    android:backgroundTint="#00695C"
                    android:textColor="#ffff"/>

                <Button
                    android:id="@+id/btn_cancel"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:text="cancel"
                    android:backgroundTint="#00695C"
                    android:textColor="#ffff"/>

            </LinearLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

</RelativeLayout>

