<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E5EDEC"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Service_List">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp">


            <androidx.cardview.widget.CardView
                android:id="@+id/sno52"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="fill"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:layout_column="0"
                android:layout_row="0"
                app:cardElevation="4dp"
                android:backgroundTint="#F1F4F4"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="sno:"
                        android:id="@+id/txtSNo"
                        android:textSize="20dp"
                        android:layout_marginTop="5dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="username:"
                        android:id="@+id/txtUsername"
                        android:textSize="20sp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtvehicleno"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="vehicleno:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtcontact1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="contact1:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtcontact2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="contact2:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtimei"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="imei:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtserialno"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="serialno:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtproblem_category"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="problem_category:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtplace"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="place:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtproduct_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="product_type:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtproblem"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="problem:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtvehicle_avail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="vehicle_avail:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtdt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="dt:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtreass_reason"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="reassign_reason:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtreass_date"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="reassign_date:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtreass_count"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="reassign_count:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtfitter_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="fitter_name:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtfitter_accept"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="fitter_accept_time:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtfitter_completion"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="completion_time:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtcompliant"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="compliant:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtsolution"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="solution:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtcash_collected"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="cash_collected:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/txtcancelreason"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="cancel_reason:"
                        android:textSize="20dp"
                        android:layout_marginTop="10dp"
                        android:textStyle="bold"/>

                </LinearLayout>

            </androidx.cardview.widget.CardView>



    </RelativeLayout>


</ScrollView>